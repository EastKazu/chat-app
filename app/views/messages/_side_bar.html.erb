<div class = side_bar_header>
  <div class = account_name>
    <%= link_to current_user.name, edit_user_path(current_user) %>
  </div>
  <div class = create_chat>
    <%= link_to "チャットを作成する", new_room_path, class:'create_chat_text'%>
  </div>
</div>

<div class = rooms>
  <% current_user.rooms.each do |room| %>
    <div class="room">
      <div class="room-name">
        <%= link_to room.name, room_messages_path(room) %>
      </div>
    </div>
  <% end %>
</div>